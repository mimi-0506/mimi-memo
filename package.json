{
  "name": "mimi-memo",
  "author": "mimi",
  "private": true,
  "version": "0.0.0",
  "description": "mimi-memo",
  "main": "dist/main/main.js",
  "scripts": {
    "kill": "node ./scripts/kill-port.js",
    "dev": "npm-run-all kill build:main --parallel start:vite start:electron:dev",
    "start:vite": "vite",
    "start:electron": "wait-on http://localhost:5173 && npm run electron",
    "start:electron:dev": "wait-on http://localhost:5173 && cross-env NODE_ENV=dev electron dist/main/main.js",
    "build:main": "tsc -p ./tsconfig.main.json",
    "build:ui": "vite build",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "npm-run-all build:main build:ui forge:make",
    "forge:make": "electron-forge make"
  },
  "win": {
    "target": "nsis",
    "sign": false,
    "certificateFile": null,
    "certificatePassword": null
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "date-fns": "^4.1.0",
    "electron-is-dev": "^3.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "emotion-reset": "^3.0.1",
    "firebase": "^11.6.0",
    "jotai": "^2.12.2",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.21.0",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.1.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "wait-on": "^8.0.3"
  }
}
